{% block widget_header %}{% endblock %}

{% block widget_title %}
    <h1 class="card-title">
        {{ title|trans }}
    </h1>
{% endblock widget_title %}

{% block widget_body %}
    {{ html|raw }}
{% endblock widget_body %}

{% block widget_footer %}{% endblock %}

{% block stylesheets %}
    {% for cssTag in cssTags %}
        {{ encore_entry_link_tags(cssTag) }}
    {% endfor %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/lledashboard/dashboard/print.js') }}" type="text/javascript"></script>
    {% for jsTag in jsTags %}
        {{ encore_entry_script_tags(jsTag) }}
    {% endfor %}
{% endblock %}
